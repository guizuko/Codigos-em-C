#include <stdio.h>
#include <math.h>
int main (void){
        double X1,X2,Y1,Y2,TOTALX,TOTALY,TOTAL; 
        scanf ("%lf %lf", &X1,&Y1);
        scanf ("%lf %lf", &X2,&Y2);
        TOTALX=(X2-X1)*(X2-X1); 
        TOTALY=(Y2-Y1)*(Y2-Y1); 
        TOTAL=sqrt(TOTALX+TOTALY);
        printf("%.4lf\n",TOTAL);
        system ("pause");
        return 0;
    }
